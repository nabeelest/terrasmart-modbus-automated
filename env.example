# Modbus Automated Environment Configuration
# Copy this file to .env and update with your actual values

# GraphQL API Configuration
GRAPHQL_URL=https://192.168.12.71/graphql
TIMEOUT_MS=8000

# Authentication Tokens
# These are required for API access to the modbus service
ACCESS_TOKEN=your_access_token_here
XSRF_TOKEN=your_xsrf_token_here
_XSRF_COOKIE=your_xsrf_cookie_here

# Optional: Direct cookie override (if needed)
COOKIE=your_cookie_here

# Site Configuration
# Default site IP/hostname for modbus operations
SITE=192.168.12.71

# Development/Testing
# Set to true for verbose logging
VERBOSE=false
